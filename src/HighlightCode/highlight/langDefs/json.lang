Description="JSON"

Keywords={
   { Id=1,
    Regex=[[ \{|\} ]],
   },
   { Id=2,
    Regex=[=[ \[|\] ]=],
   },
   { Id=3,
    Regex=[[ (\".+?\")\: ]],
   },
}

Strings = {
  Delimiter=[["]]
}

Comments = {
   { Block=true,
     Nested=false,
     Delimiter = { [[\/\*]], [[\*\/]] }
   },
   {
     Block=false,
     Delimiter = { [[//]] }
   }
}

IgnoreCase=false

PreProcessor = {
  Prefix=[[#]],
  Continuation="\\",
}

Operators=[[\(|\)|\,|\;|\.|\:|\&|<|>|\!|=|\/|\*|\%|\+|\-|\~|\||\^]]